{
  "name": "select-icon",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:docs": "npx compodoc -p projects/angular-material-extensions/select-icon/tsconfig.lib.json --hideGenerator --disableCoverage -d dist/browser/doc/",
    "deploy": "ng deploy --no-build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run select-icon:serve-ssr",
    "serve:ssr": "node dist/select-icon/server/main.js",
    "build:ssr": "ng build --prod && ng run select-icon:server:production",
    "postinstall": "ngcc",
    "serve:prerender": "cd dist/browser && http-server",
    "prerender": "ng run select-icon:prerender",
    "prerender:ghpages": "ng run select-icon:prerender_ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/platform-server": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@nguniversal/express-engine": "^11.0.0",
    "angulartics2": "^10.0.0",
    "express": "^4.15.2",
    "ngx-markdown": "^10.1.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@schematics/angular": "^9.1.12",
    "@schematics/update": "^0.901.12",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.2",
    "protractor": "~7.0.0",
    "release-it": "^14.0.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
